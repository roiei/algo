


class Solution:
    def findDuplicates(self, nums: [int]) -> [int]:
        res = []

        for i, num in enumerate(nums):
            if nums[abs(num) - 1] < 0:
                res += abs(num),
            else:
                nums[abs(num) - 1] *= -1
            
        return res
        

print([2, 3] == Solution().findDuplicates(nums = [4,3,2,7,8,2,3,1]))

